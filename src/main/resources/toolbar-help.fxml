<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="50.0" prefWidth="1030.0" stylesheets="@styles/toolbar.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Toolbar.ToolbarHelp">
    <children>
        <Button fx:id="toolbarHelp_languageButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="111.0" styleClass="toolbar-button" text="%toolbarHelp.languageButton">
            <HBox.margin>
                <Insets left="1.0" top="1.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="toolbarHelp_aboutButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="102.0" styleClass="toolbar-button" text="%toolbarHelp.aboutButton">
            <HBox.margin>
                <Insets left="1.0" top="1.0" />
            </HBox.margin>
        </Button>
    </children>
</HBox>
