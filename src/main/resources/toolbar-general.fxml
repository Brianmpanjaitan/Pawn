<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="50.0" prefWidth="1030.0" stylesheets="@styles/toolbar.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Toolbar.ToolbarGeneral">
    <children>
        <Button fx:id="toolbarGeneral_manageItemsButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="129.0" styleClass="toolbar-button" text="%toolbarGeneral.manageItemsButton">
            <HBox.margin>
                <Insets left="1.0" top="1.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="toolbarGeneral_newCategoryButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="111.0" styleClass="toolbar-button" text="%toolbarGeneral.newCategoryButton">
            <HBox.margin>
                <Insets left="1.0" top="1.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="toolbarGeneral_editCategoryButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="111.0" styleClass="toolbar-button" text="%toolbarGeneral.editCategoryButton" />
    </children>
</HBox>
