<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="50.0" prefWidth="1030.0" stylesheets="@styles/toolbar.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Toolbar.ToolbarCustomer">
    <children>
        <Button fx:id="toolbarCustomer_newCustomerButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="129.0" styleClass="toolbar-button" text="%toolbarCustomer.newCustomerButton">
            <HBox.margin>
                <Insets left="1.0" top="1.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="toolbarCustomer_editCustomerButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="119.0" styleClass="toolbar-button" text="%toolbarCustomer.editCustomerButton">
            <HBox.margin>
                <Insets left="1.0" top="1.0" />
            </HBox.margin>
        </Button>
      <Button fx:id="toolbarCustomer_viewCustomerButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="119.0" styleClass="toolbar-button" text="%toolbarCustomer.viewCustomerButton" />
    </children>
</HBox>
