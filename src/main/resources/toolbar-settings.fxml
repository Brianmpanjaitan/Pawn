<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="50.0" prefWidth="1030.0" stylesheets="@styles/toolbar.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Toolbar.ToolbarSettings">
    <children>
        <Button fx:id="toolbarSettings_newUserButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="100.0" styleClass="toolbar-button" text="%toolbarSettings.newUser">
            <HBox.margin>
                <Insets left="1.0" top="1.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="toolbarSettings_userSelectionButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="125.0" styleClass="toolbar-button" text="%toolbarSettings.userSelection">
            <HBox.margin>
                <Insets left="1.0" top="1.0" />
            </HBox.margin>
        </Button>
    </children>
</HBox>
