<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="607.0" prefWidth="896.0" stylesheets="@styles/edit-customer.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Toolbar.Customer.EditCustomer">
   <children>
      <VBox alignment="CENTER" prefHeight="496.0" prefWidth="725.0" spacing="20.0">
         <children>
            <Label fx:id="editCustomer_titleLabel" contentDisplay="RIGHT" prefHeight="47.0" prefWidth="838.0" style="-fx-font-size: 16px; -fx-font-weight: bold;" text="%editCustomer.titleLabel" />
            <HBox prefHeight="255.0" prefWidth="758.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="352.0" prefWidth="664.0" spacing="10.0">
                     <children>
                        <Label fx:id="editCustomer_promptLabel" prefHeight="33.0" prefWidth="340.0" text="%editCustomer.promptLabel" />
                        <ChoiceBox fx:id="editCustomer_customerChoiceBox" prefHeight="25.0" prefWidth="340.0" styleClass="custom-choicebox" />
                        <Label fx:id="editCustomer_nameLabel" prefHeight="33.0" prefWidth="340.0" text="%editCustomer.nameLabel" />
                        <TextField fx:id="editCustomer_customerNameField" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="340.0" styleClass="field-box">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </TextField>
                        <Label fx:id="editCustomer_phoneLabel" prefHeight="33.0" prefWidth="340.0" text="%editCustomer.phoneLabel" />
                        <TextField fx:id="editCustomer_phoneNumberField" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="340.0" styleClass="field-box" />
                        <Label fx:id="editCustomer_addressLabel" prefHeight="33.0" prefWidth="340.0" text="%editCustomer.addressLabel" />
                        <TextField fx:id="editCustomer_addressField" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="340.0" styleClass="field-box" />
                        <Label fx:id="editCustomer_genderLabel" prefHeight="33.0" prefWidth="340.0" text="%editCustomer.genderLabel" />
                        <ChoiceBox fx:id="editCustomer_genderChoiceBox" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="340.0" styleClass="custom-choicebox" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="352.0" prefWidth="664.0" spacing="10.0">
                     <children>
                        <Label fx:id="createCustomer_dobLabel" prefHeight="33.0" prefWidth="340.0" text="%createCustomer.dob" />
                        <DatePicker fx:id="editCustomer_dobPicker" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="340.0" styleClass="field-box" />
                        <Label fx:id="createCustomer_statusLabel" prefHeight="33.0" prefWidth="340.0" text="%createCustomer.status" />
                        <ChoiceBox fx:id="editCustomer_statusChoiceBox" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="340.0" styleClass="custom-choicebox" />
                        <Label fx:id="createCustomer_entryLabel" prefHeight="33.0" prefWidth="340.0" text="%createCustomer.entryDate" />
                        <DatePicker fx:id="editCustomer_entryPicker" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="340.0" styleClass="field-box" />
                        <Label fx:id="createCustomer_exitLabel" prefHeight="33.0" prefWidth="340.0" text="%createCustomer.exitDate" />
                        <DatePicker fx:id="editCustomer_exitPicker" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="340.0" styleClass="field-box" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <VBox alignment="CENTER" prefHeight="145.0" prefWidth="896.0" spacing="10.0">
               <children>
                  <Label fx:id="createCustomer_notesLabel" prefHeight="26.0" prefWidth="627.0" text="%createCustomer.notes" />
                  <TextArea fx:id="editCustomer_notesField" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="93.0" prefWidth="629.0" styleClass="field-box" />
               </children>
            </VBox>
            <HBox alignment="CENTER" prefHeight="70.0" prefWidth="664.0" spacing="10.0">
               <Button fx:id="editCustomer_confirmButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" styleClass="confirm-button" text="%editCustomer.confirmButton" />
               <Button fx:id="editCustomer_deleteButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" styleClass="default-button" text="%editCustomer.deleteButton" />
               <Button fx:id="editCustomer_cancelButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" styleClass="default-button" text="%editCustomer.cancelButton" />
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
